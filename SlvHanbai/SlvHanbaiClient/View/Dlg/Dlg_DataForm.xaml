<custom:ExChildWindow x:Class="SlvHanbaiClient.View.Dlg.Dlg_DataForm"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           xmlns:custom="clr-namespace:SlvHanbaiClient.Class.UI"
           xmlns:Utl="clr-namespace:SlvHanbaiClient.View.UserControl.DataForm.Sales"
           xmlns:theme="clr-namespace:SlvHanbaiClient.View.Dlg.Theme"
           mc:Ignorable="d"
           Height="610" 
           Width="550" 
           HasCloseButton="False"
           Loaded="ExChildWindow_Loaded" 
           FontSize="12" 
           KeyUp="ExChildWindow_KeyUp" 
           Unloaded="ExChildWindow_Unloaded" 
           xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" IsTabDefualMove="False">
    <!--HasCloseButton="False"-->

    <Grid x:Name="LayoutRoot" Background="White">
        <Grid x:Name="GridMaster">
            <Utl:Utl_DataFormOrder x:Name="UtlDataForm" />
            <theme:Dlg_ThemeShinyBlue x:Name="dlgThemeShinyBlue" />
            <theme:Dlg_ThemeTwilightBlue x:Name="dlgThemeTwilightBlue" />
        </Grid>
    </Grid>
</custom:ExChildWindow>